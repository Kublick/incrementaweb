---
type Props = {
  source: string;
  width?: number;
  height?: number;
};

const { width = 640, height = 360, source } = Astro.props as Props;
---

<script type="module" src="/node_modules/youtube-video-js/dist/youtube-video.js"
></script>

<youtube-video width={width} height={height} src={source} />
